/////////////////////////////////////////////////////////////////////////////
//
// Menu
//

IDC_SIMPLEWIN MENU DISCARDABLE 
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&Open...\tCtrl+O",            IDM_OPEN
        MENUITEM "&Close\tCtrl+W",              IDM_CLOSE
        MENUITEM SEPARATOR
        MENUITEM "E&xit\tCtrl+Q",               IDM_EXIT
    END
    POPUP "&View"
    BEGIN
        MENUITEM "100%",                        IDM_100
        MENUITEM "Show &All\tCtrl+A",           IDM_VIEWALL
        MENUITEM SEPARATOR
        MENUITEM "Zoom &In",                    IDM_ZOOMIN
        MENUITEM "Zoom &Out",                   IDM_ZOOMOUT
        MENUITEM SEPARATOR
        MENUITEM "Full &Screen\tCtrl+F",        IDM_FULLSCREEN
        MENUITEM SEPARATOR
        MENUITEM "&High Quality\tCtrl+H",       IDM_HIGHQ
    END
    POPUP "&Control"
    BEGIN
        MENUITEM "&Play",                       IDM_PLAY
        MENUITEM "&Rewind\tCtrl+R",             IDM_REWIND
        MENUITEM SEPARATOR
        MENUITEM "&Step Forward\t->",           IDM_FORWARD
        MENUITEM "&Step Back\t<-",              IDM_BACK
        MENUITEM SEPARATOR
        MENUITEM "&Loop",                       IDM_LOOP
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "About Flash Player 4...",     IDM_ABOUT
    END
END

